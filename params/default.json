{
    "model": {
        "in_channels": 4,
        "name": "resnet18",
        "num_layers": 2,
        "admm": true,
        "rho": 1,
        "iters": 20
    },
    "dataset": {   
        "size": [32, 32]
    },
    "criterion": "mse_loss",
    "solver": "Adam",
    "learning_rate": 0.001,
    "beta1": 0.9,
    "beta2": 0.999,
    "eps": 1e-06,
    "weight_decay": 1e-06,
    "policy": "StepLR",
    "step_size": 50,
    "gamma": 0.5,
    "max_epoch": 1,
    "batch_size": 64,
    "num_workers": 8,
    "target_devices": [0],
    "num_nodes": 1
}

